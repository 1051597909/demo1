<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lxh.mapper.informationMapper">
    <insert id = "insertinformation">
            insert into information(id,name,name1,age,sex,education,phonenumber,scholl,professional)
            values (#{id},#{name},#{name1},#{age},#{sex},#{education},#{phonenumber},#{scholl},#{professional});
    </insert>
</mapper>